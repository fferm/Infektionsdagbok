<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="se.fermitet.android.infektionsdagbok"
    android:versionCode="3"
    android:versionName="1.1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="19" />

    <application
        android:name="se.fermitet.android.infektionsdagbok.app.InfektionsdagbokApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="se.fermitet.android.infektionsdagbok.Questionnaire"
            android:label="@string/app_name"
            android:screenOrientation="userPortrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name="se.fermitet.android.infektionsdagbok.receiver.NotificationAlarmReceiver" >
        </receiver>

        <activity
            android:name="se.fermitet.android.infektionsdagbok.activity.Export"
            android:label="Export"
            android:screenOrientation="userPortrait" >
        </activity>
    </application>

</manifest>
